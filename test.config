# URL to AsyncAPI spec of IDM
export ASYNCAPI_FILE_NAME="events.yaml"
export DUT_ASYNCAPI_V0_URL="https://$DUT_ADDRESS/resources/v0/$ASYNCAPI_FILE_NAME"
export DUT_ASYNCAPI_V1_URL="https://$DUT_ADDRESS/resources/v1/$ASYNCAPI_FILE_NAME"
